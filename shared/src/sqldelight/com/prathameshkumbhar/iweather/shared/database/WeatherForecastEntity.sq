CREATE TABLE WeatherForecastEntity (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    temperature REAL,
    windspeed REAL,
    winddirection REAL,
    time TEXT,
    is_day INTEGER
);

selectLatest:
SELECT * FROM WeatherForecastEntity
ORDER BY id DESC
LIMIT 1;

insertWeatherForecast:
INSERT INTO WeatherForecastEntity(temperature, windspeed, winddirection, time, is_day)
VALUES (?, ?, ?, ?, ?);